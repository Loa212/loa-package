{
  "name": "loa-package-demo",
  "version": "1.0.15",
  "description": "A demo package template",
  "license": "MIT",
  "keywords": [
    "demo",
    "typescript"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "check-format": "prettier --check .",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "format": "prettier --write .",
    "lint": "tsc --noEmit",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci",
    "bump": "bumpp",
    "ci-release": "changeset publish",
    "test": "vitest run"
  },
  "homepage": "https://github.com/Loa212/loa-package",
  "bugs": {
    "url": "https://github.com/Loa212/loa-package/issues"
  },
  "author": "Loa212 <hello@loa212.com> (https://loa212.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Loa212/loa-package.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "bumpp": "^9.7.1"
  }
}
